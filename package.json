{
  "name": "Fast-Food-Fast-Nodejs",
  "version": "1.0.0",
  "description": "A food booking and delivery service for a restaurant",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm-run-all --parallel clean build migrate",
    "start": "node dist/server.js",
    "dev:start": "npm run dev && cross-env NODE_ENV=development npm run start",
    "dev:start:watch": "nodemon --watch src --exec 'npm run dev:start'",
    "build": "babel ./src -d dist",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "migrate:test": "sequelize db:migrate:undo:all && npm run migrate",
    "postinstall": "npm run dev",
    "test-env": "cross-env NODE_ENV=test",
    "test": "npm run test-env && npm run migrate:test && npm run test-report",
    "test-report": "npm run test-env && nyc --reporter=html --reporter=text mocha src/test",
    "test:watch": "nodemon --watch src/test --exec npm run test-env npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "env": "sync-dotenv"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run env"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Johnsonojo/Fast-Food-Fast-Nodejs.git"
  },
  "keywords": [],
  "author": "Johnson Ojo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Johnsonojo/Fast-Food-Fast-Nodejs/issues"
  },
  "homepage": "https://github.com/Johnsonojo/Fast-Food-Fast-Nodejs#readme",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.6.0",
    "@sendgrid/mail": "^6.4.0",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.18.4"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.6",
    "cross-env": "^5.2.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.5",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "sequelize-cli": "^5.5.1",
    "sinon": "^7.4.2",
    "sinon-chai": "^3.3.0",
    "sync-dotenv": "^2.2.1"
  }
}
